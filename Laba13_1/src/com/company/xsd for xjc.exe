<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://spring.io/guides/gs-producing-web-service"
           targetNamespace="http://spring.io/guides/gs-producing-web-service" elementFormDefault="qualified">

    <xs:element name="getSubjectRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="getSubjectResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="subject" type="tns:subject"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="subject">
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="audience" type="xs:int"/>
                <xs:element name="time" type="xs:int"/>
                <xs:element name="group" type="tns:string"/>
            </xs:sequence>
        </xs:complexType>
